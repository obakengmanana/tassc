{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getTasks, deleteTask } from '../services/taskService';\nexport default {\n  name: 'TaskList',\n  data() {\n    return {\n      tasks: []\n    };\n  },\n  async created() {\n    try {\n      const tasks = await getTasks();\n      this.tasks = tasks;\n    } catch (error) {\n      console.error('Error fetching tasks:', error);\n    }\n  },\n  methods: {\n    viewDetails(id) {\n      this.$router.push(`/task/${id}`);\n    },\n    editTask(id) {\n      this.$router.push(`/edit-task/${id}`);\n    },\n    async removeTask(id) {\n      try {\n        await deleteTask(id);\n        this.tasks = this.tasks.filter(task => task.id !== id);\n      } catch (error) {\n        console.error('Error deleting task:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["getTasks","deleteTask","name","data","tasks","created","error","console","methods","viewDetails","id","$router","push","editTask","removeTask","filter","task"],"sources":["D:\\Assessments\\tassc\\client-side\\src\\views\\TaskList.vue"],"sourcesContent":["<template>\r\n    <div class=\"task-list-container\">\r\n      <div class=\"header\">\r\n        <h1>Tasks</h1>\r\n        <router-link to=\"/add-task\" class=\"btn-primary\">Add Task</router-link>\r\n      </div>\r\n      <table class=\"task-table\" v-if=\"tasks.length\">\r\n        <thead>\r\n          <tr>\r\n            <th>Task Name</th>\r\n            <th>Description</th>\r\n            <th>Status</th>\r\n            <th>Due Date</th>\r\n            <th>Actions</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"task in tasks\" :key=\"task.id\">\r\n            <td>{{ task.name }}</td>\r\n            <td>{{ task.description }}</td>\r\n            <td>{{ task.status }}</td>\r\n            <td>{{ task.dueDate }}</td>\r\n            <td>\r\n              <button @click=\"viewDetails(task.id)\">View Details</button>\r\n              <button @click=\"editTask(task.id)\">Edit</button>\r\n              <button @click=\"removeTask(task.id)\">Delete</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <p v-else>No tasks available</p>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { getTasks, deleteTask } from '../services/taskService';\r\n  \r\n  export default {\r\n    name: 'TaskList',\r\n    data() {\r\n      return {\r\n        tasks: [],\r\n      };\r\n    },\r\n    async created() {\r\n      try {\r\n        const tasks = await getTasks();\r\n        this.tasks = tasks;\r\n      } catch (error) {\r\n        console.error('Error fetching tasks:', error);\r\n      }\r\n    },\r\n    methods: {\r\n      viewDetails(id) {\r\n        this.$router.push(`/task/${id}`);\r\n      },\r\n      editTask(id) {\r\n        this.$router.push(`/edit-task/${id}`);\r\n      },\r\n      async removeTask(id) {\r\n        try {\r\n          await deleteTask(id);\r\n          this.tasks = this.tasks.filter(task => task.id !== id);\r\n        } catch (error) {\r\n          console.error('Error deleting task:', error);\r\n        }\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n\r\n  <style scoped>\r\n  .task-list-container {\r\n    max-width: 800px;\r\n    margin: 50px auto;\r\n    padding: 20px;\r\n    border: 1px solid #eaeaea;\r\n    border-radius: 10px;\r\n    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n    font-family: Arial, sans-serif;\r\n  }\r\n  \r\n  .header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .task-table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n  }\r\n  \r\n  .task-table th, .task-table td {\r\n    padding: 10px;\r\n    border-bottom: 1px solid #eaeaea;\r\n    text-align: left;\r\n  }\r\n  \r\n  .task-table th {\r\n    background-color: #f9f9f9;\r\n  }\r\n  \r\n  .task-table td button {\r\n    background-color: #000;\r\n    color: #fff;\r\n    padding: 5px 10px;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .task-table td button:hover {\r\n    background-color: #333;\r\n  }\r\n  \r\n  .btn-primary {\r\n    background-color: #000;\r\n    color: #fff;\r\n    padding: 10px 15px;\r\n    border: none;\r\n    border-radius: 5px;\r\n    text-decoration: none;\r\n  }\r\n  \r\n  .btn-primary:hover {\r\n    background-color: #333;\r\n  }\r\n  </style>\r\n    "],"mappings":";AAmCE,SAASA,QAAQ,EAAEC,UAAS,QAAS,yBAAyB;AAE9D,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMD,KAAI,GAAI,MAAMJ,QAAQ,CAAC,CAAC;MAC9B,IAAI,CAACI,KAAI,GAAIA,KAAK;IACpB,EAAE,OAAOE,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;IAC/C;EACF,CAAC;EACDE,OAAO,EAAE;IACPC,WAAWA,CAACC,EAAE,EAAE;MACd,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,SAASF,EAAE,EAAE,CAAC;IAClC,CAAC;IACDG,QAAQA,CAACH,EAAE,EAAE;MACX,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,cAAcF,EAAE,EAAE,CAAC;IACvC,CAAC;IACD,MAAMI,UAAUA,CAACJ,EAAE,EAAE;MACnB,IAAI;QACF,MAAMT,UAAU,CAACS,EAAE,CAAC;QACpB,IAAI,CAACN,KAAI,GAAI,IAAI,CAACA,KAAK,CAACW,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACN,EAAC,KAAMA,EAAE,CAAC;MACxD,EAAE,OAAOJ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAC9C;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
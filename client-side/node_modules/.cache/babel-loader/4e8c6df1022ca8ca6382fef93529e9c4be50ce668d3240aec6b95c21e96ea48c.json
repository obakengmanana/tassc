{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { register } from '../services/authService';\nexport default {\n  name: 'UserRegister',\n  data() {\n    return {\n      name: '',\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    async handleRegister() {\n      try {\n        // Calling the register service function with user data\n        const {\n          token\n        } = await register({\n          name: this.name,\n          email: this.email,\n          password: this.password\n        });\n\n        // Storing the token in localStorage (or any secure storage)\n        localStorage.setItem('token', token);\n\n        // Redirecting the user to the home page or dashboard after successful registration\n        this.$router.push('/');\n      } catch (error) {\n        // Handling errors (e.g., displaying error messages)\n        console.error('Registration error:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["register","name","data","email","password","methods","handleRegister","token","localStorage","setItem","$router","push","error","console"],"sources":["D:\\Assessments\\tassc\\client-side\\src\\views\\UserRegister.vue"],"sourcesContent":["<template>\r\n  <div class=\"register-container\">\r\n    <h1>Register</h1>\r\n    <form @submit.prevent=\"handleRegister\">\r\n      <div class=\"form-group\">\r\n        <label for=\"name\">Name</label>\r\n        <input v-model=\"name\" type=\"text\" id=\"name\" placeholder=\"Name\" required />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"email\">Email</label>\r\n        <input v-model=\"email\" type=\"email\" id=\"email\" placeholder=\"Email\" required />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"password\">Password</label>\r\n        <input v-model=\"password\" type=\"password\" id=\"password\" placeholder=\"Password\" required />\r\n      </div>\r\n      <button type=\"submit\" class=\"btn-primary\">Register</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { register } from '../services/authService';\r\n\r\nexport default {\r\n  name: 'UserRegister',\r\n  data() {\r\n    return {\r\n      name: '',\r\n      email: '',\r\n      password: '',\r\n    };\r\n  },\r\n  methods: {\r\n    async handleRegister() {\r\n      try {\r\n        // Calling the register service function with user data\r\n        const { token } = await register({ name: this.name, email: this.email, password: this.password });\r\n        \r\n        // Storing the token in localStorage (or any secure storage)\r\n        localStorage.setItem('token', token);\r\n\r\n        // Redirecting the user to the home page or dashboard after successful registration\r\n        this.$router.push('/');\r\n      } catch (error) {\r\n        // Handling errors (e.g., displaying error messages)\r\n        console.error('Registration error:', error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.register-container {\r\n  max-width: 400px;\r\n  margin: 50px auto;\r\n  padding: 20px;\r\n  border: 1px solid #eaeaea;\r\n  border-radius: 10px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n  text-align: center;\r\n  font-family: Arial, sans-serif;\r\n}\r\n\r\n.register-container h1 {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.register-container .form-group {\r\n  margin-bottom: 15px;\r\n  text-align: left;\r\n}\r\n\r\n.register-container label {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.register-container input {\r\n  width: 100%;\r\n  padding: 10px;\r\n  border: 1px solid #eaeaea;\r\n  border-radius: 5px;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.btn-primary {\r\n  width: 100%;\r\n  padding: 10px;\r\n  margin-top: 10px;\r\n  background-color: #000;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.btn-primary:hover {\r\n  background-color: #333;\r\n}\r\n</style>\r\n"],"mappings":";AAsBA,SAAS